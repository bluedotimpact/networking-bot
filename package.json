{
  "name": "networking-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next dev",
    "lint": "next lint",
    "test": "jest",
    "deploy:prod": "vercel pull --yes --environment=production --token=$VERCEL_TOKEN && vercel build --prod --token=$VERCEL_TOKEN && vercel deploy --prebuilt --prod --token=$VERCEL_TOKEN",
    "postinstall": "cp -n .env.template .env.local"
  },
  "dependencies": {
    "@next/font": "13.1.5",
    "@seratch_/bolt-http-runner": "^1.0.3",
    "@slack/bolt": "^3.12.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "airtable": "^0.11.6",
    "axios": "^1.2.4",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "jest": "^29.4.0",
    "next": "13.1.5",
    "node-mocks-http": "^1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/.next",
      "<rootDir>/.vercel"
    ]
  }
}
